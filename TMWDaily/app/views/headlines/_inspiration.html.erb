<div class="page-header">
	<h1>Need Inspiration ?</h2>
</div>

<div class="row">
	<div class="span6">
		<h2>Last issue's headlines <small>London</small> </h2>
		<table class="table">
			<% @last_devine_headlines.each do |headline| %>
				<tr>
					<td><%= link_to headline.name, "#" %></td>
					<td><%= headline.created_at.to_formatted_s(:rfc822) %></td>
				</tr>
			<% end %>
		</table>
	</div>
	<div class="span6">
		<h2>Twitter Trends <small>London</small> </h2>
		<table class="table">
			<% @twitter_trends.each do |twit| %>
				<tr>
					<td><%= link_to twit['name'], twit['url'] %></td>
				</tr>
			<% end %>	
		</table> 
	</div>
</div>