<div class="page-header">
	<h1>Last issue's headlines</h1>
</div>

<div class="row">
	<div class="span6">
		<table class="table">
			<thead>
				<tr>
					<th>name</th>
					<th>date</th>
				</tr>
			</thead>
			<% @headlines.each do |headline| %>
				<tr>
					<td><%= link_to headline.name, devine_headline_path(headline) %></td>
					<td><%= headline.created_at.to_formatted_s(:rfc822) %></td>
				</tr>
			<% end %>
		</table>
	</div>
</div>